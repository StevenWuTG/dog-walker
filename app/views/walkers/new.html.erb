<h1> Walker Form </h1>
<% if flash[:my_errors] %>
    <% flash[:my_errors].each do |error| %>
        <p><%= error %></p>
        <% end %>
    <% end %>

<%= form_for(@walker) do |f| %>
    <label>Walker first name:</label><br>
    <%= f.text_field :first_name %><br>
  
    <label>Walker last name:</label><br>
    <%= f.text_field :last_name %><br>

    <label>Email:</label><br>
    <%= f.email_field :email %><br>

    <label>Profile Pic url:</label><br>
    <%= f.text_field :img_url %><br>
    
  

    
    <%= f.submit %>
  <% end %>